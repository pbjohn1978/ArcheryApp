<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jchandler.archeryapp.NewSetScreen">
    <TableLayout
        android:layout_width="220dp"
        android:layout_height="match_parent"
        tools:context="com.example.jchandler.tictactoe.MainActivity">

        <TableRow
            android:layout_width="220dp"
            android:layout_height="match_parent">
            <Button
                android:layout_width="200dp"
                android:layout_height="70dp"
                android:layout_span="2"
                android:id="@+id/NewSetDoneButton"
                android:text="Done Entering Set"/>

        </TableRow>
        <TableRow>
            <TextView
                android:layout_height="50dp"
                android:padding="10dp"
                android:textSize="18dp"
                android:layout_span="2"/>
        </TableRow>
        <TableRow>
            <TextView
                android:layout_height="40dp"
                android:text="Arrow # 1: "
                android:padding="10dp"
                android:textSize="18dp"
                tools:textColor="@android:color/background_dark"
                android:textColor="@android:color/background_light" />
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:id="@+id/arrow0"
                android:inputType="number"
                android:background="@color/colorPrimary" />
        </TableRow>
        <TableRow>
            <TextView
                android:layout_height="40dp"
                android:text="Arrow # 2: "
                android:padding="10dp"
                android:textSize="18dp"
                tools:textColor="@android:color/background_dark"
                android:textColor="@android:color/background_light" />
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:id="@+id/arrow1"
                android:inputType="number"
                android:background="@color/colorPrimary" />
        </TableRow>
        <TableRow>
            <TextView
                android:layout_height="40dp"
                android:text="Arrow # 3: "
                android:padding="10dp"
                android:textSize="18dp"
                tools:textColor="@android:color/background_dark"
                android:textColor="@android:color/background_light" />
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:id="@+id/arrow2"
                android:inputType="number"
                android:background="@color/colorPrimary" />
        </TableRow>
        <TableRow>
            <TextView
                android:layout_height="40dp"
                android:text="Arrow # 4: "
                android:padding="10dp"
                android:textSize="18dp"
                tools:textColor="@android:color/background_dark"
                android:textColor="@android:color/background_light" />
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:id="@+id/arrow3"
                android:inputType="number"
                android:background="@color/colorPrimary" />
        </TableRow>
        <TableRow>
            <TextView
                android:layout_height="40dp"
                android:text="Arrow # 5: "
                android:padding="10dp"
                android:textSize="18dp"
                tools:textColor="@android:color/background_dark"
                android:textColor="@android:color/background_light" />
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:id="@+id/arrow4"
                android:inputType="number"
                android:background="@color/colorPrimary" />
        </TableRow>
        <TableRow>
            <TextView
                android:layout_height="40dp"
                android:text="Arrow # 6: "
                android:padding="10dp"
                android:textSize="18dp"
                tools:textColor="@android:color/background_dark"
                android:textColor="@android:color/background_light" />
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:id="@+id/arrow5"
                android:inputType="number"
                android:background="@color/colorPrimary" />
        </TableRow>
        <TableRow>
            <TextView
                android:layout_height="40dp"
                android:text="Arrow # 7: "
                android:padding="10dp"
                android:textSize="18dp"
                tools:textColor="@android:color/background_dark"
                android:textColor="@android:color/background_light" />
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:id="@+id/arrow6"
                android:inputType="number"
                android:background="@color/colorPrimary" />
        </TableRow>
        <TableRow>
            <TextView
                android:layout_height="40dp"
                android:text="Arrow # 8: "
                android:padding="10dp"
                android:textSize="18dp"
                tools:textColor="@android:color/background_dark"
                android:textColor="@android:color/background_light" />

            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:id="@+id/arrow7"
                android:inputType="number"
                android:background="@color/colorPrimary" />
        </TableRow>
        <TableRow>
            <TextView
                android:layout_height="40dp"
                android:text="Arrow # 9: "
                android:padding="10dp"
                android:textSize="18dp"
                tools:textColor="@android:color/background_dark"
                android:textColor="@android:color/background_light" />
            <EditText
                android:layout_width="60dp"
                android:layout_height="40dp"
                android:id="@+id/arrow8"
                android:inputType="number"
                android:background="@color/colorPrimary" />
        </TableRow>
        <TableRow>
            <TextView
                android:layout_height="40dp"
                android:text="Arrow # 10: "
                android:padding="10dp"
                android:textSize="18dp"
                tools:textColor="@android:color/background_dark"
                android:textColor="@android:color/background_light" />
            <EditText
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:inputType="number"
                android:ems="40"
                android:id="@+id/arrow9"
                tools:background="@android:color/background_light"
                android:background="@color/colorPrimary" />
        </TableRow>
    </TableLayout>

</FrameLayout>
