<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="550dp"
    android:layout_height="match_parent"
    tools:context="com.example.jchandler.archeryapp.SeePastScores">

    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.jchandler.tictactoe.MainActivity">
        <TableRow>
            <TextView
                android:layout_height="50dp"
                android:text="Sets                                                                                     "
                android:padding="10dp"
                android:textColor="@android:color/background_light" />
        </TableRow>
        <TableRow>
            <Button
                android:layout_height="70dp"
                android:layout_span="9"
                android:layout_gravity="center"
                android:id="@+id/SeePastScoresDoneButton"
                android:text="Back To Main Menu"/>
        </TableRow>
        <TableRow>
            <TextView
                android:text="asdfasdf"
                android:layout_span="9"
                android:padding="10dp"
                android:id="@+id/textForPastScoresReplaceMe"
                android:textColor="@android:color/background_light" />
        </TableRow>

    </TableLayout>
</FrameLayout>
