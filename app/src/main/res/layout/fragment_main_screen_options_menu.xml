<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jchandler.archeryapp.MainScreenOptionsMenu">

    <Button
        android:text="New Set"
        android:layout_width="200dp"
        android:layout_height="70dp"
        android:id="@+id/NEWSET" />

    <Button
        android:text="See Past Sets"
        android:layout_width="200dp"
        android:layout_height="70dp"
        android:id="@+id/SEEPASTSETS"
        android:layout_below="@+id/NEWSET"
        android:layout_alignLeft="@+id/NEWSET"
        android:layout_alignStart="@+id/NEWSET"
        android:layout_marginTop="80dp" />
</FrameLayout>
